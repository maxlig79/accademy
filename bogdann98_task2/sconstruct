from SCons.Environment import Environment

env=Environment()
env.Append(CPPFLAGS=['-g', '-fprofile-arcs', '-ftest-coverage'])
env.Program(target= 'main', source= ['main.cpp', 'DynamicStringArray.h'], LIBS=['gcov'])
env.Program(target= 'TestDynamicStringArray', source= ['TestDynamicStringArray.cpp', 'DynamicStringArray.o'], LIBS=['gcov', 'boost_unit_test_framework'])